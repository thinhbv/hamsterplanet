@model MyWeb.Models.MenuModel

<div class="col-lg-3 col-md-12 ftlink-li">
    <h2 class="ft-title">@Model.parent.Name</h2>
    <a data-toggle="collapse" href="#collapse-Information" class="ft-title">
        <span>@Model.parent.Name</span>
        <i class="fa fa-angle-down"></i>
    </a>
    <ul class="footer-sublist collapse" id="collapse-Information" data-parent="#ft-parent">

        @foreach (var item in Model.childs)
        {
            <li class="ftsublink-li">
                <a href="@item.Link" title="@item.Name" class="ft-sublink">@item.Name</a>
            </li>
        }
    </ul>
</div>