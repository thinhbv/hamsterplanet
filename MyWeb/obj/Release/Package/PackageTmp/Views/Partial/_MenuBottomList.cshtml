@model List<MyWeb.Entities.Page>

<div class="col-lg-4 col-md-12 ftlink-li">
    <ul class="footer-sublist collapse" id="collapse-Information" data-parent="#ft-parent">

        @foreach (MyWeb.Entities.Page item in Model)
        {
            <li class="ftsublink-li">
                <a href="@item.Link" title="@item.Name" class="ft-sublink">@item.Name</a>
            </li>
        }
    </ul>
</div>